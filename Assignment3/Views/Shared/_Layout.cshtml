<!DOCTYPE html>
<html>

<head>
    <title>@ViewBag.Title - Assignment 3 Blog</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/DefaultStyles.css" />
</head>

<body>
    <div class="container wrapper">
        <header id="pageHeader" class="row">
            <img class="col-3" src="/img/heavysigh.jpg" alt="Heavy sigh while doing dishes" />
            <div class="col-9 productTitle">Assignment 3</div>
        </header>
        <div class="contentBody">
            <div class="row">
                <nav class="col-12 col-sm-4">
                    <ul class="list-group">
                        <li class="list-group-item">
                            <a asp-action="" asp-controller="">Add Blog Post</a>
                        </li>
                        
                    </ul>
                </nav>
                <main class="col-12 col-sm-8">
                    @RenderBody()
                </main>
            </div>
        </div>
    </div>
</body>

</html>